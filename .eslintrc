{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": ["warn", "single"],
    "no-console": "warn",
    "no-unused-vars": "warn",
    "semi": "warn"
  }
}
